
@{
    ViewData["Title"] = "Achievements";
    Layout = "~/Views/Shared/_OuterLayout.cshtml";
    string currentActionName = ViewContext.RouteData.Values["action"].ToString();
}

<!-- Feature header -->
<section class="feature_section feature_img">
    <div class="container">
        <h1>Achievements</h1>
        <span id="CurrentName" style="display:none;">@currentActionName</span>
    </div>
</section>
<!--/ End Hero Slider -->
<!-- Board of Directors Table -->
<section class="bod_section ISO_certifate">
    <div class="container">

        <div class="row">

            <div class="col-md-6">
                <div class="certificate_box">
                    <img src="../assets1/images/ESIC.jpg" alt="Certificate Image" style="width: 100%;" />
                </div>
            </div>
            <div class="col-md-6">
                <div class="certificate_box">
                    <img src="../assets1/images/achivment-1.jpg" alt="Certificate Image" style="width: 100%;" />
                </div>
            </div>
            <div class="col-md-6">
                <div class="certificate_box">
                    <img src="../assets1/images/appreciation.jpg" alt="Certificate Image" style="width: 100%;" />
                </div>
            </div>
        </div>


    </div>
</section>
<!-- /End Board of Directors Tabler -->

@section Scripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/APPjs/Common.js"></script>
    <script type="text/javascript">
            $(document).ready(function () {

              GetUpdatedRecords($("#CurrentName").html());
        })


    </script>
}

