                        <div class="form-group">
                            <label for="cont_name">OTP भरें</label>
                            <input type="password" maxlength="6" autocomplete="off" class="form-control NumberOnly" id="txtOTP">
                            <span style="color:red">OTP is valid for only ten minute.</span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <a class="btn btn-primary" id="btnVerify" href="#" cssclass="btn btn-primary" onclick="OnBeginVerify()">Verify & NEXT</a>
            </div>
        </div>
    </div>
</div>

@section Scripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/APPjs/Common.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            GetUpdatedRecords("frmContractor");
            Bindcategory()
        });


        function Bindcategory() {
            $('#ddlCategory').empty();
            $.ajax({
                type: "GET",
                url: "/Home/BindMasterCategory",
                data: {},
                async: true,
                success: function (response) {


                    $('#ddlCategory').append('<option value="0">--Select Category--</option>');
                    for (var i = 0; i < response.data.length; i++) {
                        $('#ddlCategory').append('<option value="' + response.data[i].Id + '">' + response.data[i].categoryName + '</option>');

                    }
                },

                failure: function (response) {
                    $('#ddlCategory').append('<option value="0">--Select Category--</option>');
                },
                error: function (response) {
                    $('#ddlCategory').append('<option value="0">--Select Category--</option>');
                }
            });
        }

        function Bindsubcategory() {
            $('#ddlSubCategory').empty();
            var categoryId = $('#ddlCategory').val();
            if (categoryId == 0) {
                $('#ddlSubCategory').empty();
                $('#ddlSubCategory').append('<option value="0">--Select Sub Category--</option>');
                return false;
            }

            $.ajax({
                type: "GET",
                url: "/Home/BindMasterSubCategory",
                data: { CategoryId: categoryId},
                async: true,
                success: function (response) {


                    $('#ddlSubCategory').append('<option value="0">--Select Sub Category--</option>');
                    for (var i = 0; i < response.data.length; i++) {
                        $('#ddlSubCategory').append('<option value="' + response.data[i].Id + '">' + response.data[i].SubCategoryName + '</option>');

                    }
                },

                failure: function (response) {
                    $('#ddlSubCategory').append('<option value="0">--Select Sub Category--</option>');
                },
                error: function (response) {
                    $('#ddlSubCategory').append('<option value="0">--Select Sub Category--</option>');
                }
            });
        }

        function Next() {
            var s = $('#chktc').prop("checked");
            if ($('#chktc').prop("checked") === true) {
                $('#btnpopup').click();
            }
            else {
                alert('Please click on checkbox to agree the terms and conditions of UPPCL!');
            }
        }
        function OnBegin() {
            debugger;
            if ($('#ddlCategory').val() === "0") {
                alert('Please select Category.');
                return false;
            }
            else if ($('#ddlSubCategory').val() === "0") {
                alert('Please select sub Category.');
                return false;
            }
            else if ($('#txtmobile').val() === "" || $('#txtmobile').val() === "0") {
                alert('Please enter valid Mobile Number.');
                return false;
            }
            else {
                 $('#btnSubmit').html('&nbsp;<img src="/assets/images/Ajax-loader.gif"  margin-right: 10px;" id="imgloader" alt="Loading"/>')
                 $('#btnSubmit').attr('disabled', 'disabled');
                $.get('@Url.Action("CheckContractorMobileNew","Home")', { Category: $('#ddlCategory').val(), Mobile: $('#txtmobile').val(), SubCategory: $('#ddlSubCategory').val() },
                     function (data) {
                           debugger;
                         if (data.status === 0) {
                              $('#btnSubmit').html('NEXT')
                                 $('#btnSubmit').attr('disabled', false);
                         }
                         if (data.status === 1) {
                             if (data.innerResult[0].isexists === "Success") {
                                 //$('#btnpopup1').click();
                                 //$('#btnclose').click();
                                 //$('#btnSubmit').html('NEXT')
                                 //$('#btnSubmit').attr('disabled', false);
                                   location.href = '/home/frmContractorNew';
                             }
                             else {
                                 alert(data.innerResult[0].isexists);
                                 $('#btnSubmit').html('NEXT')
                                 $('#btnSubmit').attr('disabled', false);
                                 return false;
                             }
                         }
                    }
                );

                //return true;
            }
        }
        function OnBeginVerify() {
            if ($('#txtOTP').val() === "" || $('#txtOTP').val() === "0") {
                alert('Please enter valid OTP.');
                return false;
            }
            else {
                 $('#btnVerify').html('&nbsp;<img src="/assets/images/Ajax-loader.gif"  margin-right: 10px;" id="imgloader1" alt="Loading"/>')
                 $('#btnVerify').attr('disabled', 'disabled');
                 $.get('@Url.Action("ContractorVerifyOTP","Home")', { OTP: $('#txtOTP').val() },
                     function (data) {
                         if (data === "Success") {
                             alert('OTP का सत्यापन सफलतापूर्वक हो गया हैं| दिए गए फॉर्म को भरें एवं रजिस्ट्रेशन प्रक्रिया पूरी करें|');
                             location.href = '/home/frmContractor';
                         }
                             else {
                                 alert('Either OTP is wrong or expired. Please try again.');
                                 $('#btnVerify').html('Verify & NEXT')
                                 $('#btnVerify').attr('disabled', false);
                                 return false;
                             }
                         }
                );
            }
        }
    </script>
}