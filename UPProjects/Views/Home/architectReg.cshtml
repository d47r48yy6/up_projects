@model UPProjects.Models.Architecture
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "architectReg";
    Layout = "~/Views/Shared/_OuterLayout.cshtml";
    //Layout = "~/Views/Shared/_OuterLayout-old.cshtml";
    string currentActionName = ViewContext.RouteData.Values["action"].ToString();
}
<!-- Feature header -->
<section class="feature_section feature_img">
    <div class="container">
        <h1>Architecture Registration Form</h1>
        <span id="CurrentName" style="display:none;">@currentActionName</span>
    </div>
</section>
<!--/ End Hero Slider -->
<!-- Board of Directors Table -->
<section class="bod_section">
    <div class="container">
        <!-- <div class="title">
               <h3>ठेकेदारों के रजिस्ट्रेशन हेतु आवेदन फार्म</h3>
           </div>

        -->
        <form action="SubmitArc" method="post" enctype="multipart/form-data" data-ajax="true" data-ajax-method="ture" data-ajax-begin="OnBegin" data-ajax-complete="onComplete"
