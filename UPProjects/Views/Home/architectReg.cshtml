@section Scripts{
    <script src="~/APPjs/Common.js"></script>
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script type="text/javascript">
        $(document).ready(function () {
            $(".calender").datepicker(
                {
                    dateFormat: 'dd/mm/yy',
                    maxDate: new Date()
                });
            $(".calender1").datepicker(
                {
                    dateFormat: 'dd/mm/yy'

                });

            GetUpdatedRecords($("#CurrentName").html());
        });
    </script>
    <script type="text/javascript">
        var OnBegin = function () {
            if ($('#Photo').val() !== '') {
                var filetender = $('#Photo').get(0);
                var FileSize = filetender.files[0].size;;
                var path = $('#Photo').val();
                var FileSizeKB = (FileSize / 1024).toFixed(2);
                var ext = path.substring(path.lastIndexOf(".") + 1, path.length).toLowerCase();
                if (ext == "jpg" || ext == "jpeg" || ext == "png") {
                    if (FileSizeKB > 1024) {
                        alert("Size of Photo (Signature & Designation) must be less than 1024KB (1MB).");
                        $('#Photo').val("");
                        $('#Photo').focus();
                        return false;
                    }
                }
                else {
                    alert("Format of Photo (Signature & Designation) must be in jpg,jpeg or png format.");
                    $('#Photo').val("");
                    $('#Photo').focus();
                    return false;
                }
            }
            if ($('#CouncilPhotoCoopy').val() !== '') {
                var filetender = $('#CouncilPhotoCoopy').get(0);
                var FileSize = filetender.files[0].size;;
                var path = $('#CouncilPhotoCoopy').val();
                var FileSizeKB = (FileSize / 1024).toFixed(2);
                var ext = path.substring(path.lastIndexOf(".") + 1, path.length).toLowerCase();
                if (ext == "jpg" || ext == "jpeg" || ext == "png" || ext == "pdf") {
                    if (FileSizeKB > 1024) {
                        alert("Size of Photo Copy of Registration with Council must be less than 1024KB (1MB).");
                        $('#CouncilPhotoCoopy').val("");
                        $('#CouncilPhotoCoopy').focus();
                        return false;
                    }
                }
                else {
                 alert("Format of Photo Copy of Registration with Council must be in jpg,jpeg,png or pdf.");
                    $('#CouncilPhotoCoopy').val("");
                    $('#CouncilPhotoCoopy').focus();
                    return false;
                }
            }
            if ($('#AnnexureAFirstYearFile').val() !== '') {
                var filetender = $('#AnnexureAFirstYearFile').get(0);
                var FileSize = filetender.files[0].size;;
                var path = $('#AnnexureAFirstYearFile').val();
                var FileSizeKB = (FileSize / 1024).toFixed(2);
                var ext = path.substring(path.lastIndexOf(".") + 1, path.length).toLowerCase();
                if (ext == "jpg" || ext == "jpeg" || ext == "png" || ext == "pdf") {
                    if (FileSizeKB > 1024) {
                        alert("Size of file of Project's Value must be less than 1024KB (1MB).");
                        $('#AnnexureAFirstYearFile').val("");
                        $('#AnnexureAFirstYearFile').focus();
                        return false;
                    }
