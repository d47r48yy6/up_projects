@model UPProjects.Models.ContractorNewData
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "frmContractorNew";
    Layout = "~/Views/Shared/_OuterLayout.cshtml";
    string currentActionName = ViewContext.RouteData.Values["action"].ToString();
    <span id="CurrentName" style="display:none;">@currentActionName</span>
}
<section class="feature_section feature_img">
    <div class="container">
        <h1>Contractor Registration Form</h1>
    </div>
</section>

<section class="bod_section">
    <div class="container">

        <form class="forms-sample" method="post" asp-action="frmContractorNewSubmit"  enctype="multipart/form-data"
              data-ajax="true" data-ajax-method="POST" 
              data-ajax-begin="onBegin" data-ajax-complete="onComplete"
              data-ajax-failure="onFailed" data-ajax-success="onSuccess">


            <input type="hidden" asp-for="SubCategoryId"   value="@ViewBag.SubCategory "/>
            <input type="hidden" asp-for="CategoryId"   value="@ViewBag.Category "/>
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <fieldset>
                <legend>Application Form For Registration of Contractors</legend>
                <div class="row">
                    <div class="col-xl-6 col-lg-6 col-md-6 col-sm-6 col-xs-12">
                        <div class="form-group">
                            <label asp-for="FirmName">Name of The Contractor/Name of The Firm<span style="color:red">*</span></label>
                            <input type="text" asp-for="FirmName" autocomplete="off" class="form-control" id="FirmName">
                            <span asp-validation-for="FirmName" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-xl-6 col-lg-6 col-md-6 col-sm-6 col-xs-12">
                        <div class="form-group">
                            <label asp-for="ApplicantName">Name and Designation of The Applicant (Proprietor/Partner/Director)<span style="color:red">*</span></label>
                            <input type="text" asp-for="ApplicantName" autocomplete="off" class="form-control" id="ApplicantName">
                            <span asp-validation-for="ApplicantName" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xl-6 col-lg-6 col-md-6 col-sm-6 col-xs-12">
                        <div class="form-group">
                            <label asp-for="ApplicantPhoto">Applicant's Photo<span style="color:red">*</span></label>
                            <input type="file" id="ApplicantPhoto" asp-for="ApplicantPhoto" class="form-control">
                            <span asp-validation-for="ApplicantPhoto" class="text-danger"></span>
                            <span class="text-danger" style="font-size:12px;">(Max File Size : 1 MB , File Format : jpg , jpeg ,png only)</span>

                        </div>
                    </div>
                    <div class="col-xl-6 col-lg-6 col-md-6 col-sm-6 col-xs-12">
                        <div class="form-group">

                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <h6>Address</h6>

                    </div>
                </div>
                <div class="row">
                    <div class="col-xl-6 col-lg-6 col-md-6 col-sm-6 col-xs-12">
                        <div class="form-group">

                            <label asp-for="OfficeAddress">Office<span style="color:red">*</span></label>
                            <input type="text" asp-for="OfficeAddress" autocomplete="off" class="form-control" id="OfficeAddress">
                            <span asp-validation-for="OfficeAddress" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-xl-6 col-lg-6 col-md-6 col-sm-6 col-xs-12">
                        <div class="form-group">
                            <label asp-for="ResidenceAddress">Residence<span style="color:red">*</span></label>
                            <input type="text" asp-for="ResidenceAddress" autocomplete="off" class="form-control" id="ResidenceAddress">
                            <span asp-validation-for="ResidenceAddress" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <h6>Phone Number</h6>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xl-4 col-lg-4 col-md-4 col-sm-4 col-xs-12">
                        <div class="form-group">
                            <label asp-for="OfficePhone">Office<span style="color:red">*</span></label>
                            <input type="text" asp-for="OfficePhone" autocomplete="off" class="form-control" id="OfficePhone">
                            <span asp-validation-for="OfficePhone" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-xl-4 col-lg-4 col-md-4 col-sm-4 col-xs-12">
                        <div class="form-group">
                            <label asp-for="ResidencePhone">Residence</label>
