                        <div class="form-group">
                            <label asp-for="SecurityMatureDate">Maturity Date<span style="color:red">*</span></label>
                            <input type="text" asp-for="SecurityMatureDate" readonly autocomplete="off" class="form-control calender1" id="SecurityMatureDate">
                            <span asp-validation-for="SecurityMatureDate" class="text-danger"></span>
                        </div>
