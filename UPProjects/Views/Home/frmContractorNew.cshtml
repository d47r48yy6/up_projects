            </fieldset>
        </form>
    </div>
</section>


@section Scripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/APPjs/Common.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $(".calender").datepicker(
                {
                    dateFormat: 'dd/mm/yy',
                    maxDate: new Date()
                });
            $(".calender1").datepicker(
                {
                    dateFormat: 'dd/mm/yy'

                });
            GetUpdatedRecords($("#CurrentName").html());


        });
    </script>
    <script type="text/javascript">
        var onBegin = function () {
            if ($('#ApplicantPhoto').val() !== '') {
                var filetender = $('#ApplicantPhoto').get(0);
                var FileSize = filetender.files[0].size;;
                var path = $('#ApplicantPhoto').val();
                var FileSizeKB = (FileSize / 1024).toFixed(2);
                var ext = path.substring(path.lastIndexOf(".") + 1, path.length).toLowerCase();
                if (ext == "jpg" || ext == "jpeg" || ext == "png") {
                    if (FileSizeKB > 1024) {
                        alert("The size of the applicant's photo should be less than 1024KB (1MB).");
                        $('#ApplicantPhoto').val("");
                        $('#ApplicantPhoto').focus();
                        return false;
                    }
                }
                else {
                    alert("The applicant's photo type should be jpg, jpeg or png.");
                    $('#ApplicantPhoto').val("");
                    $('#ApplicantPhoto').focus();
                    return false;
                }
            }
            if ($('#PANFile').val() !== '') {
                var filetender = $('#PANFile').get(0);
                var FileSize = filetender.files[0].size;;
                var path = $('#PANFile').val();
                var FileSizeKB = (FileSize / 1024).toFixed(2);
                var ext = path.substring(path.lastIndexOf(".") + 1, path.length).toLowerCase();
                if (ext == "jpg" || ext == "jpeg" || ext == "png" || ext == "pdf") {
                    if (FileSizeKB > 1024) {
                        alert("The size of PAN should be less than 1024 KB (1 MB).");
                        $('#PANFile').val("");
                        $('#PANFile').focus();
                        return false;
                    }
                }
                else {
                    alert("The file type of PAN should be jpg, jpeg, png or pdf only.");
                    $('#PANFile').val("");
                    $('#PANFile').focus();
                    return false;
                }
            }
            if ($('#GSTFile').val() !== '') {
                var filetender = $('#GSTFile').get(0);
                var FileSize = filetender.files[0].size;;
                var path = $('#GSTFile').val();
                var FileSizeKB = (FileSize / 1024).toFixed(2);
                var ext = path.substring(path.lastIndexOf(".") + 1, path.length).toLowerCase();
                if (ext == "jpg" || ext == "jpeg" || ext == "png" || ext == "pdf") {
                    if (FileSizeKB > 1024) {
                        alert("The file size of GST should be less than 1024 KB (1 MB).");
                        $('#GSTFile').val("");
                        $('#GSTFile').focus();
                        return false;
                    }
                }
                else {
                    alert("The file type of GST should be jpg, jpeg, png or pdf only.");
                    $('#GSTFile').val("");
                    $('#GSTFile').focus();
                    return false;
                }
            }

            if ($('#ToolsPlantsFile').val() !== '') {
                var filetender = $('#ToolsPlantsFile').get(0);
