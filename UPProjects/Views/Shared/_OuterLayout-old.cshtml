    $(document).ready(function () {


        $.ajax({

            type: "GET",
            url: "/Home/GetLastestCMAnnouncement",
            data: { Id: 0 },
            success: function (data) {

                var ext = data[0].FileName.substring(data[0].FileName.lastIndexOf(".") + 1, data[0].FileName.length).toLowerCase();
                $("a.cm").attr("href", data[0].FileName);
                $("a.cm").attr("download", "CMAnnouncement" + "." + ext);


            }
        })
        $.ajax({

            type: "GET",
            url: "/Home/GetLastestMonthlyCompileReport",
            data: { Id: 0 },
            success: function (data) {

                var ext = data[0].FileName.substring(data[0].FileName.lastIndexOf(".") + 1, data[0].FileName.length).toLowerCase();
                $("a.mcr").attr("href", data[0].FileName);


            }
        })
        InsertVisitorCount();

    });

    function InsertVisitorCount() {

        var url = "/Home/insertVisitor";
        $.getJSON(url, function (data) {

            console.log(1);


        })
        GetVisitorCount();

    }

    function GetVisitorCount() {

        var url = "/Home/GetVisitor";
        $.getJSON(url, function (data) {
            console.log(1);
            var result = parseInt(data[0].visitor) + 10000;

            $("#VisitorCount").html(result);

        })
    }

</script>
